{
    "name": "makinacorpus/drupal-sf-int",
    "type": "drupal-module",
    "description": "Symfony better integration for Drupal 8",
    "homepage": "http://github.com/makinacorpus/drupal-sf-int",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau@makina-corpus.com"
        },
        {
            "name": "Makina Corpus",
            "homepage": "http://makina-corpus.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\Drupal\\Sf\\" : "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^5.1"
    },
    "bin": [
        "bin/console"
    ],
    "require": {
        "psr/log": "1.*",
        "symfony/config": "^3.4|~4.0",
        "symfony/dependency-injection": "^3.4|~4.0",
        "symfony/event-dispatcher": "^3.4|~4.0",
        "symfony/expression-language": "^3.4|~4.0",
        "symfony/http-foundation": "^3.4|~4.0",
        "symfony/http-kernel": "^3.4|~4.0",
        "symfony/routing": "^3.4|~4.0",
        "symfony/security": "^3.4|~4.0",
        "symfony/translation": "^3.4|~4.0",
        "symfony/yaml": "^3.4|~4.0"
    },
    "suggest": {
        "jdorn/sql-formatter": "Prettifies SQL queries in the WebProfiler debug pane"
    }
}
